        "mapped_dirs": [["tmp", "./tmp"]],
        "logger_enabled": true
      },
      {
        "name": "fdb_data",
        "path": "artifacts/fdb_data.wasm",
        "mem_page_count": 100,
        "logger_enabled": true
      },
      {
        "name": "fdb_dht",
        "path": "artifacts/fdb_dht.wasm",
        "mem_page_count": 100,
        "logger_enabled": true
      },
      {
        "name": "fdb_facade",
        "path": "artifacts/fdb_facade.wasm",
        "mem_page_count": 100,
        "logger_enabled": true
      }
    ]
  }
